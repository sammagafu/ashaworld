{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'Customers',\n\n  data() {\n    return {\n      isOpen: true,\n      fullname: '',\n      email: '',\n      phonenumber: '',\n      address: '',\n      client: []\n    };\n  },\n\n  methods: {\n    openModal() {\n      this.isOpen = !this.isOpen;\n    },\n\n    closeModal() {\n      this.isOpen = !this.isOpen;\n    },\n\n    createClinet() {\n      const data = {\n        'clients': this.fullname,\n        'phone_number': this.phonenumber,\n        'clientsemail': this.email,\n        'address': this.address\n      };\n      axios.post('pos/client/', data).then(response => {\n        this.order = response.data;\n        console.log('order :>> ', response.data);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    getClinet() {\n      axios.get('pos/client').then(response => {\n        this.client = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  },\n\n  mounted() {\n    this.getClinet();\n  }\n\n};","map":{"version":3,"mappings":"AAqLI,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,WADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,MAAM,EAAE,IADL;MAEHC,QAAQ,EAAE,EAFP;MAGHC,KAAK,EAAE,EAHJ;MAIHC,WAAW,EAAE,EAJV;MAKHC,OAAO,EAAE,EALN;MAMHC,MAAK,EAAI;IANN,CAAP;EAQH,CAXU;;EAYXC,OAAO,EAAE;IACLC,SAAS,GAAG;MACR,KAAKP,MAAL,GAAc,CAAC,KAAKA,MAApB;IACH,CAHI;;IAILQ,UAAU,GAAG;MACT,KAAKR,MAAL,GAAc,CAAC,KAAKA,MAApB;IACH,CANI;;IAOLS,YAAY,GAAG;MACX,MAAMV,IAAG,GAAI;QACT,WAAW,KAAKE,QADP;QAET,gBAAgB,KAAKE,WAFZ;QAGT,gBAAgB,KAAKD,KAHZ;QAIT,WAAW,KAAKE;MAJP,CAAb;MAMAP,KAAK,CAACa,IAAN,CAAW,aAAX,EAA0BX,IAA1B,EACKY,IADL,CACUC,QAAO,IAAK;QACd,KAAKC,KAAL,GAAaD,QAAQ,CAACb,IAAtB;QACAe,OAAO,CAACC,GAAR,CAAY,YAAZ,EAA0BH,QAAQ,CAACb,IAAnC;MACH,CAJL,EAIOiB,KAJP,CAIaC,KAAI,IAAK;QACdH,OAAO,CAACC,GAAR,CAAYE,KAAZ;MACH,CANL;IAOH,CArBI;;IAsBLC,SAAS,GAAE;MACPrB,KAAK,CAACsB,GAAN,CAAU,YAAV,EAAwBR,IAAxB,CAA6BC,QAAO,IAAK;QACrC,KAAKP,MAAL,GAAcO,QAAQ,CAACb,IAAvB;MACH,CAFD,EAEGiB,KAFH